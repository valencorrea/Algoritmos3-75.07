Object subclass: #Club	instanceVariableNames: 'nombre recaudacionTotal socios tarifaSocio tarifaSocioVip'	classVariableNames: ''	package: 'tp0.5-Modelo'!!Club methodsFor: 'initialization' stamp: 'valen 4/27/2020 23:16'!initialize 		recaudacionTotal := 0.	socios := OrderedCollection new.! !!Club methodsFor: 'accessing' stamp: 'valen 4/26/2020 22:45'!recaudacionTotal	^ recaudacionTotal. ! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/28/2020 21:15'!agregarSocioVIP: unSocio	unSocio establecerTarifa: 700.	socios add: unSocio. 	! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/29/2020 01:55'!recibirPagoDe: unNombreSocio	| unSocio |	unSocio := socios detect: [ :unSocioRegistrado | (unNombreSocio = unSocioRegistrado nombre) ].	unSocio registrarPago.	recaudacionTotal := recaudacionTotal + unSocio tarifa.! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/28/2020 23:21'!nombresSocios	| nombres |	nombres := socios collect: [ :socio | socio nombre ].	^ nombres asSet.! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/26/2020 23:20'!establecerTarifaSocioVIP: unaTarifa 	tarifaSocioVip := unaTarifa. 	! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/28/2020 21:16'!agregarSocio: unSocio	unSocio establecerTarifa: 500.	socios add: unSocio.! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/26/2020 23:06'!cantidadDeSocios	^ socios size.! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/28/2020 21:55'!establecerTarifaSocio: unaTarifa	tarifaSocio := unaTarifa. ! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/26/2020 22:32'!establecerNombre: unNombre	nombre := unNombre. ! !!Club methodsFor: 'as yet unclassified' stamp: 'valen 4/29/2020 01:56'!cantidadPagadaPor: unNombre 	| unSocio |	unSocio := socios detect: [ :unSocioRegistrado | (unNombre = unSocioRegistrado nombre) ].	^ unSocio vecesPagadas * unSocio tarifa.! !Object subclass: #Socio	instanceVariableNames: 'nombre tarifa vecesPagadas'	classVariableNames: ''	package: 'tp0.5-Modelo'!!Socio methodsFor: 'as yet unclassified' stamp: 'valen 4/26/2020 22:47'!establecerNombre: unNombre 		nombre := unNombre.! !!Socio methodsFor: 'as yet unclassified' stamp: 'valen 4/26/2020 23:18'!tarifa	^ tarifa.! !!Socio methodsFor: 'as yet unclassified' stamp: 'valen 4/29/2020 01:55'!registrarPago	vecesPagadas := vecesPagadas +1.! !!Socio methodsFor: 'as yet unclassified' stamp: 'valen 4/28/2020 21:19'!establecerTarifa: unaTarifa 	tarifa := unaTarifa.! !!Socio methodsFor: 'initialization' stamp: 'valen 4/29/2020 01:54'!initialize 		vecesPagadas := 0.! !!Socio methodsFor: 'accessing' stamp: 'valen 4/28/2020 00:15'!nombre 			^ nombre.! !!Socio methodsFor: 'accessing' stamp: 'valen 4/29/2020 01:53'!vecesPagadas	^ vecesPagadas.! !