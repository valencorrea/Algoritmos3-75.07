TestCase subclass: #AlumnoTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalColegio-Tests'!!AlumnoTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:12'!test02AlumnoCon1HermanoPaga25PorCientoMenos	| unAlumno deuda otroAlumno |	unAlumno := Alumno conNombre: 'fulanito' yApellido: 'gonzales'.  	otroAlumno := Alumno conNombre: 'menganito' yApellido: 'gonzales'.	unAlumno agregarHermano: otroAlumno.	deuda := unAlumno deuda.	   	self assert: deuda equals: 75.	! !!AlumnoTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:13'!test01AlumnoHijoUnicoPagaCuotaCompleta	| unAlumno deuda |	unAlumno := Alumno conNombre: 'fulanito' yApellido: 'gonzales'.  	deuda := unAlumno deuda.	   	self assert: deuda equals: 100.    ! !TestCase subclass: #ColegioTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalColegio-Tests'!!ColegioTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 02:04'!test04dosHermanosOtroAlumnoYUnaMascotaBuenitaAdeudan325Pe	| unColegio alumno1 alumno2 alumno3 boni |	unColegio := Colegio new.		alumno1 := Alumno conNombre: 'ana' yApellido: 'gomez'.	alumno2 := Alumno conNombre: 'pepe' yApellido: 'gomez'.	alumno3 := Alumno conNombre: 'juanchi' yApellido: 'perez'.		boni := Mascota new.		alumno1 agregarHermano: alumno2.   	     	unColegio anotar: alumno1.	unColegio anotar: alumno2.	unColegio anotar: alumno3.	unColegio anotar: boni.	 	self assert: (unColegio deuda) equals: 425.! !!ColegioTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:23'!test01conCuatroAlumnosNoHermanosLaDeudaAcumuladaEsDe400Pesos	| unColegio alumno1 alumno2 alumno3 alumno4 |	unColegio := Colegio new.		alumno1 := Alumno conNombre: 'ana' yApellido: 'perez'. 	alumno2 := Alumno conNombre: 'pepe' yApellido: 'gomez'. 	alumno3 := Alumno conNombre: 'juan' yApellido: 'juarez'. 	alumno4 := Alumno conNombre: 'sofi' yApellido: 'chichixd'. 		unColegio anotar: alumno1.	unColegio anotar: alumno2.	unColegio anotar: alumno3.	unColegio anotar: alumno4.		self assert: (unColegio deuda) equals: 400. 	! !!ColegioTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:29'!test02conDosAlumnosHermanosLaDeudaAcumuladaEsDe175Pesos	| unColegio alumno1 alumno2 |	unColegio := Colegio new.		alumno1 := Alumno conNombre: 'ana' yApellido: 'perez'. 	unColegio anotar: alumno1.	     	alumno2 := Alumno conNombre: 'pepe' yApellido: 'gomez'. 	alumno2 agregarHermano: alumno1.    	unColegio anotar: alumno2.	self assert: (unColegio deuda) equals: 175.! !!ColegioTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:53'!test03conTresAlumnosHermanosLaDeudaAcumuladaEsDe175Pesos	| unColegio alumno1 alumno2 alumno3 |	unColegio := Colegio new.		alumno1 := Alumno conNombre: 'ana' yApellido: 'gomez'.	alumno2 := Alumno conNombre: 'pepe' yApellido: 'gomez'.	alumno3 := Alumno conNombre: 'juanchi' yApellido: 'gomez'.   	alumno1 agregarHermano: alumno2.	alumno1 agregarHermano: alumno3.		 	unColegio anotar: alumno1.	unColegio anotar: alumno2.	unColegio anotar: alumno3.	self assert: (unColegio deuda) equals: 200.! !TestCase subclass: #MascotaTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalColegio-Tests'!!MascotaTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:12'!test01unaMascotaPaga150Pe	| boni unPrecio |		boni := Mascota new.	unPrecio := boni deuda.		self assert: unPrecio equals: 150.  	! !!MascotaTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:13'!test02unaMascotaQueMordioPaga300Pe	| boni unPrecio |		boni := Mascota new.	boni morderMaestra.		unPrecio := boni deuda.		self assert: unPrecio equals: 300.  	! !