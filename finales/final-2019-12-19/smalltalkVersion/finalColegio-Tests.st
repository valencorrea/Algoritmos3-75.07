TestCase subclass: #AlumnoTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalColegio-Tests'!!AlumnoTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:12'!test02AlumnoCon1HermanoPaga25PorCientoMenos	| unAlumno deuda otroAlumno |	unAlumno := Alumno conNombre: 'fulanito' yApellido: 'gonzales'.  	otroAlumno := Alumno conNombre: 'menganito' yApellido: 'gonzales'.	unAlumno agregarHermano: otroAlumno.	deuda := unAlumno deuda.	   	self assert: deuda equals: 75.	! !!AlumnoTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:13'!test01AlumnoHijoUnicoPagaCuotaCompleta	| unAlumno deuda |	unAlumno := Alumno conNombre: 'fulanito' yApellido: 'gonzales'.  	deuda := unAlumno deuda.	   	self assert: deuda equals: 100.    ! !TestCase subclass: #MascotaTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalColegio-Tests'!!MascotaTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:12'!test01unaMascotaPaga150Pe	| boni unPrecio |		boni := Mascota new.	unPrecio := boni deuda.		self assert: unPrecio equals: 150.  	! !!MascotaTest methodsFor: 'tests' stamp: 'valcha 9/6/2020 01:13'!test02unaMascotaQueMordioPaga300Pe	| boni unPrecio |		boni := Mascota new.	boni morderMaestra.		unPrecio := boni deuda.		self assert: unPrecio equals: 300.  	! !