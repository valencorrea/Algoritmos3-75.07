TestCase subclass: #AvestruzTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalCorredores-Test'!!AvestruzTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:49'!test01AvestruzEnHieloNoPuedeCorrer	| avestruz velocidad |		avestruz := Avestruz new.		velocidad := avestruz correrEn: (PistaDeHielo new).		self assert: velocidad equals: 0.! !!AvestruzTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:52'!test02AvestruzCorreEnBarroA70KMPorHora	| avestruz velocidad |		avestruz := Avestruz new.		velocidad := avestruz correrEn: (Barro new).		self assert: velocidad equals: 70.! !!AvestruzTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:53'!test03AvestruzCorreEnPistaDeAtletismoA70KMPorHora	| avestruz velocidad |		avestruz := Avestruz new.		velocidad := avestruz correrEn: (PistaDeAtletismo new).		self assert: velocidad equals: 70.! !TestCase subclass: #GorrionTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalCorredores-Test'!!GorrionTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:35'!test02GorrionAlCamtarDevuelveSuCanto	| gorrion canto |		gorrion := Gorrion conVelocidad: 50 yCanto:'pio pio'.		canto := gorrion cantar.		self assert: canto equals: 'pio pio'.	! !!GorrionTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:30'!test01GorrionAlVolarDevuelveSuVelocidad	| gorrion velocidad |		gorrion := Gorrion conVelocidad: 50 yCanto:'pio pio'.		velocidad := gorrion volar.		self assert: velocidad equals: 50.	! !TestCase subclass: #PinguinoTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalCorredores-Test'!!PinguinoTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:44'!test03PinguinoNoPuedeCorrerEnAtletismo	| pinguino velocidad |		pinguino := Pinguino new.		velocidad := pinguino correrEn: (PistaDeAtletismo new).		self assert: velocidad equals: 0.! !!PinguinoTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:37'!test01PinguinoNadaA20KMPorHora	| pinguino velocidad |		pinguino := Pinguino new.		velocidad := pinguino nadar.		self assert: velocidad equals: 20.! !!PinguinoTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:43'!test03PinguinoNoPuedeCorrerEnBarro	| pinguino velocidad |		pinguino := Pinguino new.		velocidad := pinguino correrEn: (Barro new).		self assert: velocidad equals: 0.! !!PinguinoTest methodsFor: 'tests' stamp: 'valu 9/14/2020 20:42'!test02PinguinoCorreEnHieloA5KMPorHora	| pinguino velocidad |		pinguino := Pinguino new.		velocidad := pinguino correrEn: (PistaDeHielo new).		self assert: velocidad equals: 5.! !