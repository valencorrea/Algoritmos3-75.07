TestCase subclass: #InversionistaTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalInversionistas-Test'!!InversionistaTest methodsFor: 'tests' stamp: 'valu 9/16/2020 02:36'!test02InversionDeUnInversionistaConservadorConTipo1EsLaEsperada	| inversionista inversion |		inversionista := InversionistaConservador conTipo: (Tipo1 new) yMonto: 100.		inversion := inversionista inversion.		self assert: inversion equals: 100.! !!InversionistaTest methodsFor: 'tests' stamp: 'valu 9/16/2020 02:45'!test04InversionDeUnInversionistaArriesgadoConTipo1EsLaEsperada	| inversionista inversion |		inversionista := InversionistaArriesgado conTipo: (Tipo1 new) yMonto: 100.		inversion := inversionista inversion.		self assert: inversion equals: 300.! !!InversionistaTest methodsFor: 'tests' stamp: 'valu 9/16/2020 02:42'!test03InversionDeUnInversionistaArriesgadoConTipo0EsLaEsperada	| inversionista inversion |		inversionista := InversionistaArriesgado conTipo: (Tipo0 new) yMonto: 100.		inversion := inversionista inversion.		self assert: inversion equals: 200.! !!InversionistaTest methodsFor: 'tests' stamp: 'valu 9/16/2020 02:26'!test01InversionDeUnInversionistaConservadorConTipo0EsLaEsperada	| inversionista inversion |		inversionista := InversionistaConservador conTipo: (Tipo0 new) yMonto: 100.		inversion := inversionista inversion.		self assert: inversion equals: 200.! !