TestCase subclass: #CasaTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalCasa-Test'!!CasaTest methodsFor: 'tests' stamp: 'valu 9/13/2020 19:57'!test01agregoUnCercoYunaEstufaCuestan10100	|  casa estufa cerco precio |		casa := Casa new.	estufa := Estufa paraAmbiente: 1 precioKw: 10 yCalorias: 1000.	cerco := CercoElectrico paraMetros: 10 conPrecioKw: 10. 	casa comprarArtefacto: estufa.	casa comprarArtefacto: cerco.		precio := casa gastoTotal.		self assert: precio equals: 10100. ! !TestCase subclass: #CercoElectricoTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalCasa-Test'!!CercoElectricoTest methodsFor: 'tests' stamp: 'valu 9/13/2020 19:08'!test02CercoPara190MetrosYPrecioKw80Cuesta15200		| casa cerco precio |		casa := Casa new.	cerco := CercoElectrico paraMetros: 190 conPrecioKw: 80. 		casa comprarArtefacto: cerco.		precio := casa gastoTotal.		self assert: precio equals: 15200.! !!CercoElectricoTest methodsFor: 'tests' stamp: 'valu 9/13/2020 19:06'!test01CercoPara10MetrosYPrecioKw10Cuesta100		| casa cerco precio |		casa := Casa new.	cerco := CercoElectrico paraMetros: 10 conPrecioKw: 10. 		casa comprarArtefacto: cerco.		precio := casa gastoTotal.		self assert: precio equals: 100.! !TestCase subclass: #EstufaTest	instanceVariableNames: ''	classVariableNames: ''	package: 'finalCasa-Test'!!EstufaTest methodsFor: 'tests' stamp: 'valu 9/13/2020 18:58'!test02EstufaDe3Ambientes100PrecioKw900CaloriasCuesta270000		| casa estufa precio |		casa := Casa new.	estufa := Estufa paraAmbiente: 3 precioKw: 100 yCalorias: 900.		casa comprarArtefacto: estufa.		precio := casa gastoTotal.		self assert: precio equals: 270000.! !!EstufaTest methodsFor: 'tests' stamp: 'valu 9/13/2020 18:58'!test01EstufaDe1Ambiente10PrecioKw1000CaloriasCuesta10000		| casa estufa precio |		casa := Casa new.	estufa := Estufa paraAmbiente: 1 precioKw: 10 yCalorias: 1000.		casa comprarArtefac