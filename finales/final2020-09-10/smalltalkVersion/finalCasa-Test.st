narMadera: (Pino conKilos: 4 yMetros: 2).		casa comprarArtefacto: salamandra.		precio := casa gastoTotal.		self assert: 5130 closeTo: precio precision: precision.	! !!SalamandraTest methodsFor: 'tests' stamp: 'valu 9/13/2020 19:31'!test02UnaSalamandraDePinoDe8KgY8MetrosCuesta341		| casa salamandra precio |		casa := Casa new.	salamandra := Salamandra new.	salamandra determinarMadera: (Pino conKilos: 8 yMetros: 8).		casa comprarArtefacto: salamandra.		precio := casa gastoTotal.		self assert: 341 closeTo: precio precision: precision.		! !!SalamandraTest methodsFor: 'running' stamp: 'valu 9/13/2020 19:32'!setUp	precision := 0.001! !