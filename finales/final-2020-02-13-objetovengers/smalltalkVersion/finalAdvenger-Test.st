TestCase subclass: #AdvengersTests	instanceVariableNames: ''	classVariableNames: ''	package: 'finalAdvenger-Test'!!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 19:26'!test08IronManConArmaduraMarkISePoneArmaduraWarMachineYAtacaAThanos	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraMarkI new).		ironMan1 atacarA: thanos. 			self assert: (thanos puntosVida) closeTo: 240 precision: 0.001.	self assert: (ironMan1 puntosVida) closeTo: 100 precision: 0.001.		ironMan1 agregarArmadura: (ArmaduraWarMachine new).	ironMan1 atacarA: thanos. 		self assert: (thanos puntosVida) closeTo: 220 precision: 0.001.	self assert: (ironMan1 puntosVida) closeTo: 110 precision: 0.001.	! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 19:43'!test11ThanosConsigueLasGemasDelInfinitoYAtacaConElCuadrupleDePoder	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraMarkI new).		thanos conseguirGemasDelInfinito.	thanos atacarA: ironMan1. 		self assert: (ironMan1 puntosVida) closeTo: 40 precision: 0.001.! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 19:05'!test05IronManConArmaduraMarkIIQuita12PorCientoDeSuVidaAThanos	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraMarkII new).		ironMan1 atacarA: thanos. "ataca con 12"			self assert: (thanos puntosVida) closeTo: 238 precision: 0.001.		thanos atacarA: ironMan1. 		ironMan1 atacarA: thanos. "ataca con 12"		self assert: (thanos puntosVida) closeTo: 226 precision: 0.001.	! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 18:22'!test04IronManConArmaduraMarkIQuita10PuntosDeVidaAThanos	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraMarkI new).		ironMan1 atacarA: thanos. 		self assert: (thanos puntosVida) closeTo: 240 precision: 0.001.	! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 18:09'!test02IronManEmpiezaSiempreCon100PuntosDeVida	| ironMan1 ironMan2 ironMan3 |		ironMan1 := IronMan conArmadura: (ArmaduraMarkI new).	ironMan2 := IronMan conArmadura: (ArmaduraMarkII new).	ironMan3 := IronMan conArmadura: (ArmaduraWarMachine new).		self assert: (ironMan1 puntosVida) closeTo: 100 precision: 0.001.	self assert: (ironMan2 puntosVida) closeTo: 100 precision: 0.001.	self assert: (ironMan3 puntosVida) closeTo: 100 precision: 0.001.		! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 19:18'!test06IronManConArmaduraWarMachineAtacaAThanosYLeRoba10PuntosDeVida	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraWarMachine new).		ironMan1 atacarA: thanos. 			self assert: (thanos puntosVida) closeTo: 240 precision: 0.001.	self assert: (ironMan1 puntosVida) closeTo: 110 precision: 0.001.	! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 18:13'!test03ThanosAtacaAIronManYLeQuita15PuntosDeVida	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraMarkI new).		thanos atacarA: ironMan1. 		self assert: (ironMan1 puntosVida) closeTo: 85 precision: 0.001.		! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 19:23'!test07IronManConArmaduraMarkISePoneArmaduraMarkIIYAtacaAThanos	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraMarkI new).		ironMan1 atacarA: thanos. 			self assert: (thanos puntosVida) closeTo: 240 precision: 0.001.	self assert: (ironMan1 puntosVida) closeTo: 100 precision: 0.001.		ironMan1 agregarArmadura: (ArmaduraMarkII new).	ironMan1 atacarA: thanos. 		self assert: (thanos puntosVida) closeTo: 218 precision: 0.001.! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 18:05'!test01ThanosEmpiezaSiempreCon250PuntosDeVida	| thanos |		thanos := Thanos new.		self assert: (thanos puntosVida) closeTo: 250 precision: 0.001.	! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 19:29'!test09IronManConArmaduraMarkIISePoneLasOtrasArmadurasYAtacaAThanos	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraMarkII new).		ironMan1 atacarA: thanos. 			self assert: (thanos puntosVida) closeTo: 238 precision: 0.001.		ironMan1 agregarArmadura: (ArmaduraMarkI new).	ironMan1 atacarA: thanos. 		self assert: (thanos puntosVida) closeTo: 216 precision: 0.001.	ironMan1 agregarArmadura: (ArmaduraWarMachine new).	ironMan1 atacarA: thanos. 	self assert: (ironMan1 puntosVida) closeTo: 110 precision: 0.001.	self assert: (thanos puntosVida) closeTo: 184 precision: 0.001.! !!AdvengersTests methodsFor: 'tests' stamp: 'valcha 9/6/2020 19:36'!test10IronManConArmaduraWarMachineSePoneLasOtrasArmadurasYAtacaAThanos	| ironMan1 thanos |		thanos := Thanos new.	ironMan1 := IronMan conArmadura: (ArmaduraWarMachine new).		ironMan1 atacarA: thanos. 			self assert: (thanos puntosVida) closeTo: 240 precision: 0.001.	self assert: (ironMan1 puntosVida) closeTo: 110 precision: 0.001.		ironMan1 agregarArmadura: (ArmaduraMarkI new).	ironMan1 atacarA: thanos. 		self assert: (thanos puntosVida) closeTo: 220 precision: 0.001.	self assert: (ironMan1 puntosVida) closeTo: 120 precision: 0.001.	ironMan1 agregarArmadura: (ArmaduraMarkII new).	ironMan1 atacarA: thanos. 	self assert: (ironMan1 puntosVida) closeTo: 130 precision: 0.001.	self assert: (thanos puntosVida) closeTo: 188 precision: 0.001.! !