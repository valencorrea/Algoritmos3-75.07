Object subclass: #VehiculoTerrestre	instanceVariableNames: 'tipoVehiculo'	classVariableNames: ''	poolDictionaries: ''	category: 'ParcialAlgo3'!!VehiculoTerrestre methodsFor: 'initialization' stamp: 'PabloS 6/14/2020 11:36'!setTipoVehiculo: unTipoVehiculo	tipoVehiculo := unTipoVehiculo .! !!VehiculoTerrestre methodsFor: 'as yet unclassified' stamp: 'PabloS 6/14/2020 11:40'!romperVehiculo	tipoVehiculo := 'vehiculoAveriado'! !!VehiculoTerrestre methodsFor: 'as yet unclassified' stamp: 'PabloS 6/14/2020 11:52'!cantidadKmAlAvanzarHoras: cantidadDeHorasQueAvanza		(tipoVehiculo = 'motoneta') ifTrue: [ ^40*cantidadDeHorasQueAvanza ].	(tipoVehiculo = 'motoDeCarrera') ifTrue: [ ^240*cantidadDeHorasQueAvanza ].	(tipoVehiculo = 'vehiculoAveriadado') 						ifTrue: [ Transcript show: 'Una vehiculo averiado no recorre distancia'.									^ false].	Transcript show: 'vehiculo no implementado'.	^ false.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!VehiculoTerrestre class	instanceVariableNames: ''!!VehiculoTerrestre class methodsFor: 'as yet unclassified' stamp: 'PabloS 6/14/2020 11:30'!crearMotoneta	| unaMotoneta |	unaMotoneta := VehiculoTerrestre new.	unaMotoneta setTipoVehiculo: 'motoneta'.	^ unaMotoneta ! !